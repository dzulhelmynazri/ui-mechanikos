{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "avatar-demo",
  "type": "registry:example",
  "registryDependencies": [
    "avatar"
  ],
  "files": [
    {
      "path": "registry/new-york-v4/examples/avatar-demo.tsx",
      "content": "import {\n  Avatar,\n  AvatarFallback,\n  AvatarImage,\n} from \"@/registry/new-york-v4/ui/avatar\"\n\nexport default function AvatarDemo() {\n  return (\n    <div className=\"flex flex-row flex-wrap items-center gap-12\">\n      <Avatar>\n        <AvatarImage\n          src=\"https://github.com/dzulhelmynazri.png\"\n          alt=\"@dzulhelmynazri\"\n        />\n        <AvatarFallback>DZ</AvatarFallback>\n      </Avatar>\n      <Avatar className=\"rounded-lg\">\n        <AvatarImage\n          src=\"https://github.com/izueibrahim.png\"\n          alt=\"@izueibrahim\"\n        />\n        <AvatarFallback>DZ</AvatarFallback>\n      </Avatar>\n      <div className=\"*:data-[slot=avatar]:ring-background flex -space-x-2 *:data-[slot=avatar]:ring-2 *:data-[slot=avatar]:grayscale\">\n        <Avatar>\n          <AvatarImage\n            src=\"https://github.com/dzulhelmynazri.png\"\n            alt=\"@dzulhelmynazri\"\n          />\n          <AvatarFallback>DZ</AvatarFallback>\n        </Avatar>\n        <Avatar>\n          <AvatarImage\n            src=\"https://github.com/mrnonchalantz.png\"\n            alt=\"@mrnonchalantz\"\n          />\n          <AvatarFallback>MN</AvatarFallback>\n        </Avatar>\n        <Avatar>\n          <AvatarImage\n            src=\"https://github.com/izueibrahim.png\"\n            alt=\"@izueibrahim\"\n          />\n          <AvatarFallback>IZ</AvatarFallback>\n        </Avatar>\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}