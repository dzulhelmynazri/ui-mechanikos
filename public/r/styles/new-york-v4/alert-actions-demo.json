{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "alert-actions-demo",
  "type": "registry:example",
  "registryDependencies": [
    "alert"
  ],
  "files": [
    {
      "path": "registry/new-york-v4/examples/alert-actions-demo.tsx",
      "content": "import { Bell } from \"lucide-react\"\n\nimport {\n  Alert,\n  AlertContent,\n  AlertDescription,\n  AlertIcon,\n  AlertTitle,\n} from \"@/registry/new-york-v4/ui/alert\"\nimport { Button } from \"@/registry/new-york-v4/ui/button\"\n\nexport default function AlertActionsDemo() {\n  return (\n    <div className=\"flex w-full flex-col items-center gap-6 lg:max-w-[75%]\">\n      <Alert variant=\"warning\" close={false}>\n        <AlertIcon>\n          <Bell />\n        </AlertIcon>\n        <AlertContent>\n          <AlertTitle>Example Alert Title</AlertTitle>\n          <AlertDescription>\n            <p>\n              Insert the alert description here. This is an example of a\n              two-line message for better visual clarity.\n            </p>\n            <div className=\"space-x-3.5\">\n              <Button\n                variant=\"inverse\"\n                mode=\"link\"\n                underlined=\"solid\"\n                size=\"md\"\n              >\n                Upgrade\n              </Button>\n              <Button variant=\"inverse\" mode=\"link\" underline=\"solid\" size=\"md\">\n                Dismiss\n              </Button>\n            </div>\n          </AlertDescription>\n        </AlertContent>\n      </Alert>\n      <Alert variant=\"destructive\" appearance=\"light\" close={false}>\n        <AlertIcon>\n          <Bell />\n        </AlertIcon>\n        <AlertContent>\n          <AlertTitle>Example Alert Title</AlertTitle>\n          <AlertDescription>\n            <p>\n              Insert the alert description here. This is an example of a\n              two-line message for better visual clarity.\n            </p>\n            <div className=\"space-x-3.5\">\n              <Button\n                variant=\"foreground\"\n                mode=\"link\"\n                underlined=\"solid\"\n                size=\"md\"\n              >\n                Upgrade\n              </Button>\n              <Button\n                variant=\"foreground\"\n                mode=\"link\"\n                underline=\"solid\"\n                size=\"md\"\n              >\n                Dismiss\n              </Button>\n            </div>\n          </AlertDescription>\n        </AlertContent>\n      </Alert>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}