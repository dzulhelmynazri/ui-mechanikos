      "content": "import {\n  Avatar,\n  AvatarFallback,\n  AvatarImage,\n} from \"@/registry/new-york-v4/ui/avatar\"\nimport { Button } from \"@/registry/new-york-v4/ui/button\"\n\nexport default function AvatarDemo() {\n  return (\n    <div className=\"flex -space-x-2\">\n      <Avatar>\n        <AvatarImage\n          src=\"https://github.com/dzulhelmynazri.png\"\n          alt=\"@dzulhelmynazri\"\n          className=\"border-background border-2 hover:z-10\"\n        />\n        <AvatarFallback>DZ</AvatarFallback>\n      </Avatar>\n      <Avatar>\n        <AvatarImage\n          src=\"https://github.com/izueibrahim.png\"\n          alt=\"@izueibrahim\"\n          className=\"border-background border-2 hover:z-10\"\n        />\n        <AvatarFallback>IZ</AvatarFallback>\n      </Avatar>\n      <Avatar>\n        <AvatarImage\n          src=\"https://github.com/izueibrahim.png\"\n          alt=\"@izueibrahim\"\n          className=\"border-background border-2 hover:z-10\"\n        />\n        <AvatarFallback>IZ</AvatarFallback>\n      </Avatar>\n      <Avatar>\n        <AvatarImage\n          src=\"https://github.com/izueibrahim.png\"\n          alt=\"@izueibrahim\"\n          className=\"border-background border-2 hover:z-10\"\n        />\n        <AvatarFallback>IZ</AvatarFallback>\n      </Avatar>\n      <Button\n        variant=\"secondary\"\n        size=\"icon\"\n        shape=\"circle\"\n        className=\"border-background relative size-10 border-2 hover:z-10\"\n      >\n        +7\n      </Button>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}